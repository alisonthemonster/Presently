<resources>

    <style name="AppTheme" parent="Base.AppTheme" />

    <!-- Base application theme. -->
    <style name="Base.AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textColor">@color/text_color</item>
        <item name="android:statusBarColor">?attr/statusBarColor</item>
        <item name="colorPrimary">?attr/timelineLineColor</item>
        <item name="colorSecondary">?attr/highlightColor</item>
        <item name="android:textColorSecondary">?attr/timelineHintColor</item>
        <item name="android:colorBackground">?attr/timelineBackgroundColor</item>
        <item name="snackbarButtonStyle">@style/snackbar_button</item>

        <!--   Colors for the Timeline screen    -->
        <item name="timelineIcon">@drawable/ic_flower</item>
        <item name="timelineBackgroundColor">@color/originalBackgroundColor</item>
        <item name="timelineLineColor">@color/originalTimelineColor</item>
        <item name="timelineHeaderColor">@color/originalTimelineColor</item>
        <item name="timelineBodyColor">@color/originalTimelineColor</item>
        <item name="timelineHintColor">@color/originalHintTextColor</item>
        <item name="toolbarColor">@color/originalTimelineColor</item>
        <item name="toolbarItemColor">@color/originalBackgroundColor</item>
        <item name="presentlyLogoColor">?attr/toolbarItemColor</item>
        <item name="statusBarColor">@color/originalTimelineColor</item>
        <item name="fabColor">?attr/toolbarColor</item>
        <item name="fabTextColor">?attr/toolbarItemColor</item>

        <!--   Colors for the Entry screen    -->
        <item name="entryBackgroundColor">?attr/timelineBackgroundColor</item>
        <item name="entryHeaderColor">@color/originalTimelineColor</item>
        <item name="entryBodyColor">@color/originalTimelineColor</item>
        <item name="entryHintColor">?attr/timelineHintColor</item>

        <item name="alertDialogTheme">@style/AlertDialogTheme</item>
        <item name="highlightColor">@color/originalTimelineColor</item>
        <item name="fontFamily">@font/larsseit_medium</item>
        <item name="android:fontFamily">@font/larsseit_medium</item>
    </style>

